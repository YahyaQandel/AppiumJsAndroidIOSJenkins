    "child_process": "^1.0.2",
    "colors": "^1.4.0",
    "eslint": "^6.5.1",
    "jest": "^24.9.0",
    "metro-react-native-babel-preset": "^0.58.0",
    "mocha": "^7.1.1",
    "mocha-junit-reporter": "^1.23.3",
    "path": "^0.12.7",
